a = 1
b = 2

// A C-like if statement.
if (a < b) {
    system.println("a < b")
} else if (a > b) {
    system.println("a > b")
} else {
    system.println("a == b")
}

// A C-like switch statement.
switch (a) {
    case 1:
        system.println("a == 1")
        break
    case 2:
        system.println("a == 2")
        break
    default:
        system.println("a != 1 && a != 2")
}