f(x) = 2 * x ** 2 + x - 1

g(x) {
    return x * x
}

f1(x) ?= {
    return x
}

f2(x) := {
    return x
}

f3(x) => /{
    y = x + 1;
    return y;
}/

i32 f4(i32 $a, i32 $b) /{
    (i32.add
      (get_local $a)
      (get_local $b)
    )
}/

a ?= f1(2)
b := f2(2)
c = f3(2)
d = f4(1, 2)

system.println(f(2));
system.println(g(2));
system.println(a);
system.println(b);
system.println(c);
system.println(d);
